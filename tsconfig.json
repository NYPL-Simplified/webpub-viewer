{
  "compileOnSave": true,
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES6",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist/",
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/sw.ts"
  ]
}
